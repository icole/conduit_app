## This file must *NOT* be checked into Version Control Systems,
# as it contains information specific to your local configuration.
#
# Copy this file to local.properties and fill in your actual values.
# local.properties is already in .gitignore by default.

# Location of the SDK (will be auto-generated by Android Studio)
sdk.dir=/path/to/your/Android/sdk

# API Keys and sensitive configuration
# Replace these with your actual API keys
GOOGLE_CLIENT_ID=YOUR_GOOGLE_WEB_CLIENT_ID_HERE
STREAM_API_KEY=YOUR_STREAM_API_KEY_HERE