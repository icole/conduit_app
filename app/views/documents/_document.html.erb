<div id="<%= dom_id document %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= document.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= document.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Google drive url:</strong>
    <%= document.google_drive_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Document type:</strong>
    <%= document.document_type %>
  </div>
</div>
